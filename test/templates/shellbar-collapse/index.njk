{% extends "layout.njk" %}
{% from "./component.njk" import shell_header_collapse %}
{% from "./../format.njk" import format %}

{% set css_deps = ['fonts','helpers', 'components/popover', 'components/button', 'components/menu', 'icons'] %}

{% block content %}
<div style="background-color: #f3f4f5; margin:-20px; padding:50px;">
    <h1>Shell Header Collapse</h1>

    {% set basic_shell_header_collapse %}
        {{ shell_header_collapse(properties={ "items": data.properties.items }) }}
    {% endset %}

    {{ format(basic_shell_header_collapse) }}
</div>
{% endblock %}
