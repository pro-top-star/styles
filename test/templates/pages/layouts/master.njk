<!doctype html>
<html lang="en-us">
<head>
    <meta charset="utf-8">
    <!--[if IE]><![endif]-->
    <meta http-equiv=X-UA-Compatible content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ id }}</title>
    <style media="screen">
    {% set core_sass = 'all.scss' %}
    {{ core_sass | sass_to_css }}
    </style>
</head>
<body>

<div class="fd-ui fd-ui--{{ app.id }}">
    <div class="fd-ui__header">
        <code class="fd-has-color-inverse">&nbsp;</code>
    </div>
    <div class="fd-ui__app" id="fd-ui-app">
        {# <div class="fd-ui__sidebar" style="width: 200px;">
            <code class="fd-has-color-inverse">&nbsp;</code>
        </div>
        <main class="fd-ui__main"> #}
            {% block content %}
                  .fd-ui__app
            {% endblock %}
        {# </main> #}
    </div>
    <div class="fd-overlay" aria-hidden="true" id="fd-ui-overlay">
        .fd-overlay
    </div>
</div>

</body>
</html>
